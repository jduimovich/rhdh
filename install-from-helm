SCRIPTDIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" >/dev/null 2>&1 && pwd )" 
source $SCRIPTDIR/verify-dependencies 

NS=dance-installer
DH_NAME=dance

echo "Install from Helm" 
helm upgrade  $DH_NAME  rhtap-dance/dance --install --create-namespace --namespace $NS --values workdir/values-secret.yaml